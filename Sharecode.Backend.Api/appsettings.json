{
  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.File"],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Host": "Error",
        "Microsoft": "Error",
        "System": "Error",
        "Microsoft.AspNetCore": "Error",
        "Microsoft.AspNetCore.Authentication" : "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName"],
    "Properties": {
      "ApplicationName": "Your ASP.NET Core App"
    }
  },
  "CloudFlareKV": {
    "AuthorizationBearer": "",
    "ApiEmail" : "",
    "AccountIdentifier" : "",
    "NamespaceIdentifier" : ""
  },
  "JWT": {
    "Issuer": "",
    "Audience" : ""
  },
  "LocalDirectory" : {
    "EmailTemplates" : "D:\\ShareCode\\EmailTemplates"
  },
  "GatewayLimit": {
    "VerifyUserAccount" : 0,
    "ForgotPassword" : 0
  },
  "Frontend": {
    "Base": "https://sharecodeapp.onrender.com/"
  },
  "FileClient": {
    "ClientType" : "Local",
    "Local": {
      "FilePath" : "",
      "FileUrl": "",
      "RevokeExecutePermission" : false
    }
  },
  "AllowedHosts": "*"
}